AAL,GAAW;AAACqB,YAAAA,CAAC,EAAC,CAAH;AAAMC,YAAAA,CAAC,EAAC,CAAR;AAAWC,YAAAA,CAAC,EAAC;AAAb,WAAX;AACA,eAAKvB,GAAL,CAASqB,CAAT,GAAa2B,MAAM,CAAC3B,CAApB;AACA,eAAKrB,GAAL,CAASsB,CAAT,GAAa0B,MAAM,CAAC1B,CAApB;AACA,eAAKtB,GAAL,CAASuB,CAAT,GAAayB,MAAM,CAACzB,CAApB,CAJoB,CAMpB;;AACA4B,UAAAA,MAAM,CAACC,IAAP,CAAY,mBAAZ,EAAiC,KAAKb,EAAtC,EAA0CO,CAAC,CAACG,MAAF,CAASjD,GAAnD;AACD,SARD,MAUK;AACH;AACA,cAAIqD,CAAC,GAAG,KAAKrD,GAAL,CAASqB,CAAT,GAAa2B,MAAM,CAAC3B,CAA5B;AACA,cAAIiC,CAAC,GAAG,KAAKtD,GAAL,CAASsB,CAAT,GAAa0B,MAAM,CAAC1B,CAA5B;AACA,cAAIiC,CAAC,GAAG,KAAKvD,GAAL,CAASuB,CAAT,GAAayB,MAAM,CAACzB,CAA5B;AACA,cAAIiC,QAAQ,GAAIH,CAAC,GAACA,CAAF,GAAMC,CAAC,GAACA,CAAR,GAAYC,CAAC,GAACA,CAA9B;AAGA,cAAIC,QAAQ,GAAG,GAAf,EACE7C,OAAO,CAACC,GAAR,CAAY,YAAZ,EATC,CAUD;;AACA,eAAKZ,GAAL,CAASqB,CAAT,GAAa2B,MAAM,CAAC3B,CAApB;AACA,eAAKrB,GAAL,CAASsB,CAAT,GAAa0B,MAAM,CAAC1B,CAApB;AACA,eAAKtB,GAAL,CAASuB,CAAT,GAAayB,MAAM,CAACzB,CAApB,CAbC,CAeD;;AACF4B,UAAAA,MAAM,CAACC,IAAP,CAAY,mBAAZ,EAAiC,KAAKb,EAAtC,EAA0CS,MAA1C;AACC;AACJ;AAEF,KAtDM;AAyDPS,IAAAA,UAzDO,cAyDM;AACX,WAAKC,KAAL,CAAW,aAAX;AACD;AA3DM,GAxEI;AAuIbC,EAAAA,KAAK,EAAE;AACLC,IAAAA,OAAO,EAAE,YAAU;AAChBC,MAAAA,QAAQ,CAACC,aAAT,CAAuB,OAAvB,EAAgCC,YAAhC,CAA6C,SAA7C,EAAwD,KAAKH,OAA7D;AACF;AAHI,GAvIM;AA6IbI,EAAAA,QAAQ,EAAE;AA7IG,CA1Cf","sourcesContent":["\n\n//collections\nimport {Players} from \"../../imports/collections/players.js\";\n\n//Vue\nimport Vue from \"vue\";\nVue.config.ignoredElements = [\n  'a-scene',\n  'a-entity',\n  'a-camera',\n  'a-box',\n  'a-sky',\n  'a-sphere',\n  'a-cylinder',\n  'a-plane'\n];\n\n\n//Aframe\nimport {Aframe} from \"aframe\"\n\nAFRAME.registerComponent('listener', {\n  tick: function () {\n    this.el.emit(\"update-pos\", {pos:this.el.getAttribute('position'), rot:this.el.getAttribute('rotation')}, false);\n  }\n});\n\nAFRAME.registerComponent('teleporter', {\n  teleportTo: function (pov) {\n    this.el.object3D.position.copy(pov.position);\n    this.el.object3D.rotation.copy(new THREE.Euler(0, 0, 0));\n    console.log(this.yawObject, this.pitchObject)\n    this.yawObject = 0;\n  }\n});\n\n//Vue\nimport Object from \"./Object.vue\";\n\n\n\nexport default {\n  name: \"Scene\",\n\n  components: {\n    Object,\n  },\n\n  props:[\"active\", \"id\"],\n\n  data() {\n    \n    return {\n\n      //player latest pos\n      pos:null,\n\n\n      //cam pov\n      pov1: {\n        position:{x:0, y:1, z:3},\n        rotation:{x:-15, y:0, z:0},\n      },\n\n      pov2: {\n        position:{x:0, y:10, z:-3},\n        rotation:{x:-50, y:-100, z:0},\n      },\n\n      //player\n      player :{\n        position:\"0 1 3\",\n        rotation:\"-15 0 0\",\n      },\n\n      //objects\n      cube1: {\n        text:\"Ceci est une sphère\",\n        hovering:false,\n        position:\"2 2 2\",\n        color:\"#EF2D5E\",\n      },\n\n      cube2: {\n        hovering:false,\n        position:\"10 4 2\",\n        color:\"#ABCDEF\",\n      },\n\n      cube3: {\n        hovering:false,\n        position:\"5 0 5\",\n        color:\"#FFE44C\",\n      }\n    }\n  },\n\n  mounted(){\n  },\n\n    meteor: {\n      $subscribe: {\n        players: []\n      },\n\n      players() {\n        return Players.find({ _id: { $ne: this.id } });\n      }\n  },\n\n  components: {\n  },\n\n  methods: {\n\n    backgroundStyle(data) {\n      if (data.hovering)\n        return {'background-color':data.color}\n      else\n        return {'background-color':\"white\"}\n    },\n\n    teleport(pov) {\n      this.$refs.cam.components[\"teleporter\"].teleportTo(pov);  \n    },\n\n    updatePos(e) {\n\n      if (this.active) {\n        let newPos = e.detail.pos;\n        let newRot = e.detail.rot;\n\n        //update player pos data\n       // this.player.position = newPos.x + \" \" + newPos.y + \" \" + newPos.z; \n        //this.player.rotation = newRot.x + \" \" + newRot.y + \" \" + newRot.z; \n\n        //check if new\n        if (this.pos == null) {\n          this.pos = {x:0, y:0, z:0}\n          this.pos.x = newPos.x; \n          this.pos.y = newPos.y; \n          this.pos.z = newPos.z; \n          \n          //and send to server\n          Meteor.call('players.updatePos', this.id, e.detail.pos)\n        }\n\n        else {\n          //!! distance with no square root\n          var a = this.pos.x - newPos.x;\n          var b = this.pos.y - newPos.y;\n          var c = this.pos.z - newPos.z;\n          let distance = (a*a + b*b + c*c);\n\n        \n          if (distance > 0.1)\n            console.log(\"deplacment\")\n            //save pos\n            this.pos.x = newPos.x; \n            this.pos.y = newPos.y; \n            this.pos.z = newPos.z; \n\n            //and send to server\n          Meteor.call('players.updatePos', this.id, newPos)\n          }\n      }\n\n    },\n\n\n    enterScene() {\n      this.$emit(\"enter-scene\");\n    },\n\n  },\n\n  watch: {\n    visible: function(){\n       document.querySelector('a-box').setAttribute('visible', this.visible)\n    }\n  },\n\n  computed: {\n    \n  }\n\n  \n};\n\n"]},"sourceType":"module","hash":"2eaa6b9a3e0f007b748cfeff2c353a7d6596ab28"}
