]\n    };\n  },\n  meteor: {\n    //responsive frames collection\n    allFrames: function () {\n      return Frames.find({}, {\n        sort: {\n          createdAt: -1\n        }\n      }).fetch();\n    }\n  },\n  components: {\n    Frame: Frame\n  },\n  methods: {\n    frameOpened: function (id, children) {\n      console.log(\"opening children: \", children);\n\n      for (i in children) {\n        var parentIndex = this.openFrames.indexOf(id);\n        var childIndex = children[i].childId; //if already open\n\n        if (this.openFrames.indexOf(childIndex) != -1) continue;\n        console.log(\"splice at: \", parentIndex, \" and \", children[i].childId);\n        this.openFrames.splice(parentIndex + 1, 0, children[i].childId);\n      }\n\n      console.log(this.openFrames);\n    },\n    frameClosed: function (id, children) {\n      for (i in children) {\n        var index = this.openFrames.indexOf(children[i].childId);\n        if (index != -1) this.openFrames.splice(index, 1);\n      }\n\n      console.log(\"closing children: \" + children);\n    },\n    frameDeleted: function (id) {\n      console.log(\"deleting rame from view\");\n      var index = this.openFrames.indexOf(id);\n      if (index != -1) this.openFrames.splice(index, 1);\n    }\n  }\n});","map":{"version":3,"sources":["/home/benoit/Documents/TutoMeteor/client/components/App.vue"],"names":["Vue","module","link","v","Frame","Frames","exportDefault","name","data","openFrames","meteor","allFrames","find","sort","createdAt","fetch","components","methods","frameOpened","id","children","console","log","i","parentIndex","indexOf","childIndex","childId","splice","frameClosed","index","frameDeleted"],"mappings":"AAAA,IAAIA,GAAJ;AAAQC,MAAM,CAACC,IAAP,CAAY,KAAZ,EAAkB;AAAC,aAAQ,UAASC,CAAT,EAAW;AAACH,IAAAA,GAAG,GAACG,CAAJ;AAAM;AAA3B,CAAlB,EAA+C,CAA/C;AAAkD,IAAIC,KAAJ;AAAUH,MAAM,CAACC,IAAP,CAAY,aAAZ,EAA0B;AAAC,aAAQ,UAASC,CAAT,EAAW;AAACC,IAAAA,KAAK,GAACD,CAAN;AAAQ;AAA7B,CAA1B,EAAyD,CAAzD;AAA4D,IAAIE,MAAJ;AAAWJ