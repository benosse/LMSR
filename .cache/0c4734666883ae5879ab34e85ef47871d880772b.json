 in DB\n            Meteor.call(\"frames.remove\", this.frameId);\n\n            //also delete from app viex\n            this.$emit('deleted', this.frameId);\n        },\n  }\n\n};\n"]},"sourceType":"module","hash":"0c4734666883ae5879ab34e85ef47871d880772b"}
