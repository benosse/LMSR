\n}, 3);\nVue.config.ignoredElements = ['a-scene', 'a-entity', 'a-camera', 'a-box', 'a-sky', 'a-sphere', 'a-cylinder', 'a-plane']; //Aframe\n\nAFRAME.registerComponent('listener', {\n  tick: function () {\n    this.el.emit(\"update-pos\", {\n      pos: this.el.getAttribute('position'),\n      rot: this.el.getAttribute('rotation')\n    }, false);\n  }\n});\nAFRAME.registerComponent('teleporter', {\n  teleportTo: function (pos) {\n    console.log(\"teleporting\", this.el, pos);\n  }\n}); //Vue\n\nmodule.exportDefault({\n  name: \"Scene\",\n  components: {\n    Object: Object\n  },\n  props: [\"active\", \"id\"],\n  data: function () {\n    return {\n      //player latest pos\n      pos: null,\n      //cam pov\n      pov1: {\n        position: \"0 1 3\",\n        rotation: \"-15 0 0\"\n      },\n      pov2: {\n        position: \"0 10 -3\",\n        rotation: \"-50 -100 0\"\n      },\n      //player\n      player: {\n        position: \"0 1 3\",\n        rotation: \"-15 0 0\"\n      },\n      //objects\n      cube1: {\n        text: \"Ceci est une sphère\",\n        hovering: false,\n        position: \"2 2 2\",\n        color: \"#EF2D5E\"\n      },\n      cube2: {\n        hovering: false,\n        position: \"10 4 2\",\n        color: \"#ABCDEF\"\n      },\n      cube3: {\n        hovering: false,\n        position: \"5 0 5\",\n        color: \"#FFE44C\"\n      }\n    };\n  },\n  mounted: function () {},\n  meteor: {\n    $subscribe: {\n      players: []\n    },\n    players: function () {\n      return Players.find({\n        _id: {\n          $ne: this.id\n        }\n      });\n    }\n  },\n  components: {},\n  methods: {\n    backgroundStyle: function (data) {\n      if (data.hovering) return {\n        'background-color': data.color\n      };else return {\n        'background-color': \"white\"\n      };\n    },\n    teleport: function (pov) {\n      var cam = this.$refs.cam;\n      cam.components[\"teleporter\"].teleportTo(pov);\n      /*\n      console.log(\"moviiiiing\")\n      this.$refs.camRig.setAttribute(\"position\", pov.position);\n      this.$refs.camRig.setAttribute(\"rotation\", \"0 0.5 0\");\n      */\n    },\n    updatePos: function (e) {\n      if (this.active) {\n        var newPos = e.detail.pos;\n        var newRot = e.detail.rot; //update player pos data\n        // this.player.position = newPos.x + \" \" + newPos.y + \" \" + newPos.z; \n        //this.player.rotation = newRot.x + \" \" + newRot.y + \" \" + newRot.z; \n        //check if new\n\n        if (this.pos == null) {\n          this.pos = {\n            x: 0,\n            y: 0,\n            z: 0\n          };\n          this.pos.x = newPos.x;\n          this.pos.y = newPos.y;\n          this.pos.z = newPos.z; //and send to server\n\n          Meteor.call('players.updatePos', this.id, e.detail.pos);\n        } else {\n          //!! distance with no square root\n          var a = this.pos.x - newPos.x;\n          var b = this.pos.y - newPos.y;\n          var c = this.pos.z - newPos.z;\n          var distance = a * a + b * b + c * c;\n          if (distance > 0.1) console.log(\"deplacment\"); //save pos\n\n          this.pos.x = newPos.x;\n          this.pos.y = newPos.y;\n          this.pos.z = newPos.z; //and send to server\n\n          Meteor.call('players.updatePos', this.id, newPos);\n        }\n      }\n    },\n    enterScene: function () {\n      this.$emit(\"enter-scene\");\n    }\n  },\n  watch: {\n    visible: function () {\n      document.querySelector('a-box').setAttribute('visible', this.visible);\n    }\n  },\n  computed: {}\n});","map":{"version":3,"sources":["/home/benoit/Documents/TutoMeteor/client/components/Scene.vue"],"names":["Players","module","link","v","Vue","Aframe","Object","config","ignoredElements","AFRAME","registerComponent","tick","el","emit","pos","getAttribute","rot","teleportTo","console","log","exportDefault","name","components","props","data","pov1","position","rotation","pov2","player","cube1","text","hovering","