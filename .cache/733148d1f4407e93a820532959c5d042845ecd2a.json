CA,eAAKL,GAAL,CAAS0C,CAAT,GAAaH,MAAM,CAACG,CAApB;AACA,eAAK1C,GAAL,CAAS2C,CAAT,GAAaJ,MAAM,CAACI,CAApB;AACA,eAAK3C,GAAL,CAAS4C,CAAT,GAAaL,MAAM,CAACK,CAApB,CAbC,CAeD;;AACFC,UAAAA,MAAM,CAACC,IAAP,CAAY,mBAAZ,EAAiC,KAAKjB,EAAtC,EAA0CU,MAA1C;AACC;AACJ;AAEF,KA7DM;AAgEPY,IAAAA,UAhEO,cAgEM;AACX,WAAKC,KAAL,CAAW,aAAX;AACD;AAlEM,GAvEI;AA6IbC,EAAAA,KAAK,EAAE;AACLC,IAAAA,OAAO,EAAE,YAAU;AAChBC,MAAAA,QAAQ,CAACC,aAAT,CAAuB,OAAvB,EAAgCC,YAAhC,CAA6C,SAA7C,EAAwD,KAAKH,OAA7D;AACF;AAHI,GA7IM;AAmJbI,EAAAA,QAAQ,EAAE;AAnJG,CAvCf","sourcesContent":["\n\n//collections\nimport {Players} from \"../../imports/collections/players.js\";\n\n//Vue\nimport Vue from \"vue\";\nVue.config.ignoredElements = [\n  'a-scene',\n  'a-entity',\n  'a-camera',\n  'a-box',\n  'a-sky',\n  'a-sphere',\n  'a-cylinder',\n  'a-plane'\n];\n\n\n//Aframe\nimport {Aframe} from \"aframe\"\n\nAFRAME.registerComponent('listener', {\n  tick: function () {\n    this.el.emit(\"update-pos\", {pos:this.el.getAttribute('position'), rot:this.el.getAttribute('rotation')}, false);\n  }\n});\n\nAFRAME.registerComponent('teleporter', {\n  teleportTo: function (pos) {\n    console.log(\"teleporting\", this.el, pos);\n  }\n});\n\n//Vue\nimport Object from \"./Object.vue\";\n\n\n\nexport default {\n  name: \"Scene\",\n\n  components: {\n    Object,\n  },\n\n  props:[\"active\", \"id\"],\n\n  data() {\n    \n    return {\n\n      //player latest pos\n      pos:null,\n\n\n      //cam pov\n      pov1: {\n        position:\"0 1 3\",\n        rotation:\"-15 0 0\",\n      },\n      pov2: {\n        position:\"0 10 -3\",\n        rotation:\"-50 -100 0\",\n      },\n\n      //player\n      player :{\n        position:\"0 1 3\",\n        rotation:\"-15 0 0\",\n      },\n\n      //objects\n      cube1: {\n        text:\"Ceci est une sphère\",\n        hovering:false,\n        position:\"2 2 2\",\n        color:\"#EF2D5E\",\n      },\n\n      cube2: {\n        hovering:false,\n        position:\"10 4 2\",\n        color:\"#ABCDEF\",\n      },\n\n      cube3: {\n        hovering:false,\n        position:\"5 0 5\",\n        color:\"#FFE44C\",\n      }\n    }\n  },\n\n  mounted(){\n  },\n\n    meteor: {\n      $subscribe: {\n        players: []\n      },\n\n      players() {\n        return Players.find({ _id: { $ne: this.id } });\n      }\n  },\n\n  components: {\n  },\n\n  methods: {\n\n    backgroundStyle(data) {\n      if (data.hovering)\n        return {'background-color':data.color}\n      else\n        return {'background-color':\"white\"}\n    },\n\n    teleport(pov) {\n      let cam = this.$refs.cam;\n      \n      cam.components[\"teleporter\"].teleportTo(pov);\n      /*\n      console.log(\"moviiiiing\")\n      this.$refs.camRig.setAttribute(\"position\", pov.position);\n      this.$refs.camRig.setAttribute(\"rotation\", \"0 0.5 0\");\n      */\n    },\n\n    updatePos(e) {\n\n      if (this.active) {\n        let newPos = e.detail.pos;\n        let newRot = e.detail.rot;\n\n        //update player pos data\n       // this.player.position = newPos.x + \" \" + newPos.y + \" \" + newPos.z; \n        //this.player.rotation = newRot.x + \" \" + newRot.y + \" \" + newRot.z; \n\n        //check if new\n        if (this.pos == null) {\n          this.pos = {x:0, y:0, z:0}\n          this.pos.x = newPos.x; \n          this.pos.y = newPos.y; \n          this.pos.z = newPos.z; \n          \n          //and send to server\n          Meteor.call('players.updatePos', this.id, e.detail.pos)\n        }\n\n        else {\n          //!! distance with no square root\n          var a = this.pos.x - newPos.x;\n          var b = this.pos.y - newPos.y;\n          var c = this.pos.z - newPos.z;\n          let distance = (a*a + b*b + c*c);\n\n        \n          if (distance > 0.1)\n            console.log(\"deplacment\")\n            //save pos\n            this.pos.x = newPos.x; \n            this.pos.y = newPos.y; \n            this.pos.z = newPos.z; \n\n            //and send to server\n          Meteor.call('players.updatePos', this.id, newPos)\n          }\n      }\n\n    },\n\n\n    enterScene() {\n      this.$emit(\"enter-scene\");\n    },\n\n  },\n\n  watch: {\n    visible: function(){\n       document.querySelector('a-box').setAttribute('visible', this.visible)\n    }\n  },\n\n  computed: {\n    \n  }\n\n  \n};\n\n"]},"sourceType":"module","hash":"733148d1f4407e93a820532959c5d042845ecd2a"}
