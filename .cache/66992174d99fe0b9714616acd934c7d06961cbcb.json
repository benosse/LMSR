t Meteor reactive\n    }\n  },\n  // Meteor reactivity\n  meteor: {\n    // Subscriptions - Errors not reported spelling and capitalization.\n    $subscribe: {\n      'Frame': []\n    },\n    // A helper function to get the current time\n    currentTime: function () {\n      console.log('Calculating currentTime');\n      var t = Frame.findOne('currentTime') || {};\n      return t.time;\n    },\n    // A Minimongo cursor on the Time collection is added to the Vue instance\n    TimeCursor: function () {\n      // Here you can use Meteor reactive sources like cursors or reactive vars\n      // as you would in a Blaze template helper\n      return Time.find({}, {\n        sort: {\n          time: -1\n        }\n      });\n    }\n  }\n});","map":{"version":3,"sources":["/home/benoit/Documents/TutoMeteor/client/components/App.vue"],"names":["module","link","exportDefault","data","console","log","hello","settings","Meteor","public","methods","updateFrame","call","meteor","$subscribe","currentTime","t","Frame","findOne","time","TimeCursor","Time","find","sort"],"mappings":"AAAAA,MAAM,CAACC,IAAP,CAAY,4BAAZ;AAAAD,MAAM,CAACE,aAAP,CAKe;AACbC,EAAAA,IADa,cACN;AACLC,IAAAA,OAAO,CAACC,GAAR,CAAY,0CAAZ;AACA,WAAO;AACLC,MAAAA,KAAK,EAAE,OADF;AAELC,MAAAA,QAAQ,EAAEC,MAAM,CAACD,QAAP,CAAgBE,MAFrB,CAE+B;;AAF/B,KAAP;AAID,GAPY;AAUb;AACAC,EAAAA,OAAO,EAAE;AACPC,IAAAA,WADO,cACO;AACZP,MAAAA,OAAO,CAACC,GAAR,CAAY,kCAAZ;AACAG,MAAAA,MAAM,CAACI,IAAP,CAAY,OAAZ,EAFY,CAEmB;AAChC;AAJM,GAXI;AAmBb;AACAC,EAAAA,MAAM,EAAE;AACN;AACAC,IAAAA,UAAU,EAAE;AACV,eAAS;AADC,KAFN;AAKN;AACAC,IAAAA,WANM,cAMS;AACbX,MAAAA,OAAO,CAACC,GAAR,CAAY,yBAAZ;AACA,UAAIW,CAAC,GAAGC,KAAK,CAACC,OAAN,CAAc,aAAd,KAAgC,EAAxC;AACA,aAAOF,CAAC,CAACG,IAAT;AACD,KAVK;AAaN;AACAC,IAAAA,UAdM,cAcQ;AACZ;AACA;AACA,aAAOC,IAAI,CAACC,IAAL,CAAU,EAAV,EAAc;AACnBC,QAAAA,IAAI,EAAE;AAACJ,UAAAA,IAAI,EAAE,CAAC;AAAR;AADa,OAAd,CAAP;AAGD;AApBK;AApBK,CALf","sourcesContent":["\n\n\nimport '/imports/collections/Frame';\n\nexport default {\n  data() {\n    console.log('Sending non-Meteor data to Vue component');\n    return {\n      hello: 'World',\n      settings: Meteor.settings.public,   // not Meteor reactive\n    }\n  },\n\n\n  // Vue Methods\n  methods: {  \n    updateFrame() {\n      console.log('Calling Meteor Method UpdateTime');\n      Meteor.call('Frame');          // not Meteor reactive\n    }\n  },\n\n\n  // Meteor reactivity\n  meteor: {\n    // Subscriptions - Errors not reported spelling and capitalization.\n    $subscribe: {\n      'Frame': []\n    },\n    // A helper function to get the current time\n    currentTime () {\n      console.log('Calculating currentTime');\n      var t = Frame.findOne('currentTime') || {};\n      return t.time;\n    },\n\n    \n    // A Minimongo cursor on the Time collection is added to the Vue instance\n    TimeCursor () {\n      // Here you can use Meteor reactive sources like cursors or reactive vars\n      // as you would in a Blaze template helper\n      return Time.find({}, {\n        sort: {time: -1}\n      })\n    },\n  }\n}\n"]},"sourceType":"module","hash":"66992174d99fe0b9714616acd934c7d06961cbcb"}
