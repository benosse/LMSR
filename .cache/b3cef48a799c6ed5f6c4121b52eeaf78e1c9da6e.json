-cylinder', 'a-plane']; //Aframe\n\nAFRAME.registerComponent('listener', {\n  tick: function () {\n    this.el.emit(\"update-pos\", {\n      pos: this.el.getAttribute('position')\n    }, false);\n  }\n});\n/*\nCE QU'IL FAUT AVOIR\n\nconst mediaElement = new Audio( \"https://icecast.radiofrance.fr/francemusiqueeasyclassique-midfi.mp3\" );\nmediaElement.play();\n\naudio.srcObject = stream;\n\naudio.setMediaElementSource( mediaElement );\n\n\n\n*/\n\nmodule.exportDefault({\n  name: \"Scene\",\n  props: [\"active\", \"id\"],\n  data: function () {\n    return {\n      //player latest pos\n      pos: null,\n      //cube\n      color: \"green\",\n      visible: true,\n      hoveringCube: false,\n      //sphere\n      sphereData: {\n        text: \"Ceci est une sphère\",\n        hovering: false\n      },\n      //cube\n      cubeData: {\n        hovering: false\n      },\n      //cylinder\n      cylinderData: {\n        hovering: false\n      }\n    };\n  },\n  mounted: function () {},\n  meteor: {\n    $subscribe: {\n      players: []\n    },\n    players: function () {\n      return Players.find({\n        _id: {\n          $ne: this.id\n        }\n      });\n    }\n  },\n  components: {},\n  methods: {\n    updatePos: function (e) {\n      if (this.active) {\n        var newPos = e.detail.pos; //check if new\n\n        if (this.pos == null) {\n          this.pos = {\n            x: 0,\n            y: 0,\n            z: 0\n          };\n          this.pos.x = newPos.x;\n          this.pos.y = newPos.y;\n          this.pos.z = newPos.z; //and send to server\n\n          Meteor.call('players.updatePos', this.id, e.detail.pos);\n        } else {\n          //!! distance with no square root\n          var a = this.pos.x - newPos.x;\n          var b = this.pos.y - newPos.y;\n          var c = this.pos.z - newPos.z;\n          var distance = a * a + b * b + c * c;\n          if (distance > 0.1) console.log(\"deplacment\"); //save pos\n\n          this.pos.x = newPos.x;\n          this.pos.y = newPos.y;\n          this.pos.z = newPos.z; //and send to server\n\n          Meteor.call('players.updatePos', this.id, newPos);\n        }\n      } //send pos to server\n      //Meteor.call('players.updatePos', this.id, e.detail.pos)\n\n    },\n    enterScene: function () {\n      this.$emit(\"enter-scene\");\n    },\n    setVisibility: function () {\n      this.visible = !this.visible;\n    },\n    setBoxColor: function (newColor) {\n      this.color = newColor;\n    },\n    mouseEnterHandler: function (event) {\n      console.log(\"enter\");\n      this.hoveringCube = true;\n    },\n    mouseLeaveHandler: function (event) {\n      console.log(\"leave\");\n      this.hoveringCube = false;\n    },\n    mouseClickHandler: function (event) {\n      console.log(\"clic\");\n    },\n    //sphere\n    mouseEnterSphereHandler: function (event) {\n      console.log(\"enter\");\n      this.sphereData.visible = true;\n    },\n    mouseLeaveSphereHandler: function (event) {\n      console.log(\"leave\");\n      this.sphereData.visible = false;\n    }\n  },\n  watch: {\n    visible: function () {\n      document.querySelector('a-box').setAttribute('visible', this.visible);\n    }\n  },\n  computed: {\n    backgroundStyle: function () {\n      if (this.hoveringCube) return {\n        'background-color': this.color\n      };else return {\n        'background-color': \"white\"\n      };\n    }\n  }\n});","map":{"version":3,"sources":["/home/benoit/Documents/TutoMeteor/client/components/Scene.vue"],"names":["Players","module","link","v","Vue","Aframe","config","ignoredElements","AFRAME","registerComponent","tick","el","emit","pos","getAttribute","exportDefault","name","props","data","color","visible","hoveringCube","sphereData","text","hovering","cubeData","cylinderData","mounted","meteor","$subscribe","players","find","_id","$ne","id","components","methods","updatePos","e","active","newPos","detail","x","y","z","Meteor","call","a","b","c","distance","console","l