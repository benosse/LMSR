\n      deep: true,\n      handler: function () {\n        console.log(\"masks changed\");\n\n        if (this.frameData) {\n          var tag = this.frameData.tag;\n          console.log(\"zz\", tag, this.masks);\n          var visibility = this.masks[tag].value;\n\n          if (visibility) {\n            this.maskVisibility = \"visible\";\n          } else {\n            this.maskVisibility = \"hidden\";\n          }\n        }\n      }\n    }\n  },\n  computed: {\n    dynamicStyle: function () {\n      if (!this.frameData) return {};else {\n        return {\n          \"border-color\": \"rgb(\" + this.frameData.color.r + \",\" + this.frameData.color.g + \",\" + this.frameData.color.b + \")\"\n        };\n      }\n    },\n    hasChildren: function () {\n      if (!this.frameData) return false;\n      return this.frameData.children.length > 0;\n    },\n    dynamicMaskStyle: function () {\n      if (!this.frameData) return {\n        visibility: \"hidden\"\n      };\n      return {\n        visibility: this.maskVisibility,\n        \"background-color\": this.frameData.tag == 0 ? \"rgba(255,77,77,0.9)\" : \"rgba(51,153,255,0.9)\"\n      };\n    }\n  },\n  meteor: {\n    //responsive frames collection\n    frameData: function () {\n      return Frames.findOne({\n        _id: this.frameId\n      });\n    }\n  },\n  methods: {\n    show: function () {\n      this.isOpen = true;\n      this.$emit(\"shown\", this.frameId, this.frameData.children);\n    },\n    hide: function () {\n      this.isOpen = false;\n      this.$emit(\"hidden\", this.frameData);\n    },\n    createFrame: function () {\n      var _this = this;\n\n      Meteor.call(\"frames.insert\", this.frameId, this.frameData.color, function (error, result) {\n        _this.show();\n      });\n    },\n    deleteFrame: function () {\n      //delete in DB\n      Meteor.call(\"frames.remove\", this.frameId); //also delete from app viex\n\n      this.$emit(\"deleted\", this.frameId);\n    }\n  }\n});","map":{"versio