);\nmodule.exportDefault({\n  name: \"App\",\n  data: function () {\n    return {};\n  },\n  meteor: {\n    //responsive frames collection\n    allFrames: function () {\n      return Frames.find({}, {\n        sort: {\n          createdAt: -1\n        }\n      }).fetch();\n    }\n  },\n  components: {\n    Frame: Frame,\n    Scene: Scene\n  },\n  methods: {\n    //handling masks\n    applyMask: function (index) {\n      this.masks[index].value = !this.masks[index].value;\n    },\n    toggleEditMode: function () {\n      this.editMode = !this.editMode;\n    },\n    //handling frames\n    frameShown: function (id, children) {\n      console.log(\"opening children: \", children);\n\n      for (i in children) {\n        var parentIndex = this.openFrames.indexOf(id);\n        var childIndex = children[i].childId; //if already open\n\n        if (this.openFrames.indexOf(childIndex) != -1) continue;\n        console.log(\"splice at: \", parentIndex, \" and \", children[i].childId);\n        this.openFrames.splice(parentIndex + 1, 0, children[i].childId);\n      }\n\n      console.log(this.openFrames);\n    },\n    frameHidden: function (id) {\n      var frame = Frames.findOne(id);\n\n      if (frame) {\n        for (i in frame.children) {\n          var index = this.openFrames.indexOf(frame.children[i].childId);\n\n          if (index != -1) {\n            //trigger frameHidden event recursivly on children\n            this.frameHidden(frame.children[i].childId); //hide child\n\n            this.openFrames.splice(index, 1);\n          }\n        }\n      }\n    },\n    frameDeleted: function (id) {\n      var index = this.openFrames.indexOf(id);\n      console.log(\"deleting rame from view\", index);\n      if (index != -1) this.openFrames.splice(index, 1);\n    }\n  }\n});","map":{"version":3,"sources":["/home/benoit/Documents/TutoMeteor/client/components/App.vue"],"names":["Vue","module","link","v","Scene","Session","Frames","exportDefault","name","data","meteor","allFra