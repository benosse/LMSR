);\nvar Players;\nmodule.link(\"../../imports/collections/players.js\", {\n  Players: function (v) {\n    Players = v;\n  }\n}, 4);\nmodule.exportDefault({\n  name: \"App\",\n  components: {\n    Scene: Scene\n  },\n  data: function () {\n    return {\n      isPlayerActive: false\n    };\n  },\n  mounted: function () {\n    //remove player from server on unload\n    window.onbeforeunload = this.disconnectPlayer; //create session in DB\n\n    Meteor.call('players.insert', function (error, result) {\n      console.log(\"player created with id: \", result);\n      Session.set('id', result); //by default set user to inactive\n\n      Session.set('active', false);\n    });\n  },\n  meteor: {\n    //responsive frames collection\n    allFrames: function () {\n      return Frames.find({}, {\n        sort: {\n          createdAt: -1\n        }\n      }).fetch();\n    }\n  },\n  methods: {\n    disconnectPlayer: function () {\n      Meteor.call('players.remove', Session.get('id'));\n    },\n    setPlayerActive: function () {\n      Session.set(\"active\", true);\n      console.log(this.isPlayerActive());\n    },\n    isPlayerActive: function () {\n      console.log(\"gettt\");\n      return Session.get(\"active\");\n    },\n    //handling masks\n    applyMask: function (index) {\n      this.masks[index].value = !this.masks[index].value;\n    },\n    toggleEditMode: function () {\n      this.editMode = !this.editMode;\n    },\n    //handling frames\n    frameShown: function (id, children) {\n      console.log(\"opening children: \", children);\n\n      for (i in children) {\n        var parentIndex = this.openFrames.indexOf(id);\n        var childIndex = children[i].childId; //if already open\n\n        if (this.openFrames.indexOf(childIndex) != -1) continue;\n        console.log(\"splice at: \", parentIndex, \" and \", children[i].childId);\n        this.openFrames.splice(parentIndex + 1, 0, children[i].childId);\n      }\n\n      console.log(this.openFrames);\n    },\n    frameHidden: function (id) {\n      var frame = Frames.findOne(id);\n\n      if (frame) {\n        for (i in frame.children) {\n          var index = this.openFrames.indexOf(frame.children[i].childId);\n\n          if (index != -1) {\n            //trigger frameHidden event recursivly on children\n            this.frameHidden(frame.children[i].childId); //hide child\n\n            this.openFrames.splice(index, 1);\n          }\n        }\n      }\n    },\n    frameDeleted: function (id) {\n      var index = this.openFrames.indexOf(id);\n      console.log(\"deleting rame from view\", index);\n      if (index != -1) this.openFrames.splice(index, 1);\n    }\n  }\n});","map":{"version":3,"sources":["/home/benoit/Documents/TutoMeteor/client/components/App.vue"],"names":["Vue","module","link","v","Scene","Session","Frames","Players","exportDefault","name","components","data","isPlayerActive","mounted","window","onbeforeunload","disconnectPlayer","Meteor","call","error","result","console","log","set","meteor","allFrames","find","sort","createdAt","fetch","methods","get","setPlayerActive","applyMask","index","masks","value","toggleEditMode","editMode","frameShown","id","children","i","parentIndex","openFrames","indexOf","childIndex","childId","splice","frameHidden","frame","findOne","frameDeleted"],"mappings":"AAAA,IAAIA,GAAJ;AAAQC,MAAM,CAACC,IAAP,CAAY,KAAZ,EAAkB;AAAC,aAAQ,UAASC,CAAT,EAAW;AAACH,IAAAA,GAAG,GAACG,CAAJ;AAAM;AAA3B,CAAlB,EAA+C,CAA/C;AAAkD,IAAIC,KAAJ;AAAUH,MAAM,CAACC,IAAP,CAAY,aAAZ,EAA0B;AAAC,aAAQ,UAASC,CAAT,EAAW;AAACC,IAAAA,KAAK,GAACD,CAAN;AAAQ;AAA7B,CAA1B,EAAyD,CAAzD;AAA4D,IAAIE,OAAJ;AAAYJ,MAAM,CAACC,IAAP,CAAY,gBAAZ,EAA6B;AAACG,EAAAA,OAAO,EAAC,UAASF,CAAT,EAAW;AAACE,IAAAA,OAAO,GAACF,CAAR;AAAU;AAA/B,CAA7B,EAA8D,CAA9D;AAAiE,IAAIG,MAAJ;AAAWL,MAAM,CAACC,IAAP,CAAY,qCAAZ,EAAkD;AAACI,EAAAA,MAAM,EAAC,UAASH,CAAT,EAAW;AAACG,IAAAA,MAAM,GAACH,CAAP;AAAS;AAA7B,CAAlD,EAAiF,CAAjF;AAAoF,IAAII,OAAJ;AAAYN,MAAM,CAACC,IAAP,CAAY,sCAAZ,EAAmD;AAACK,EAAAA,OAAO,EAAC,UAASJ,CAAT,EAAW;AAACI,I