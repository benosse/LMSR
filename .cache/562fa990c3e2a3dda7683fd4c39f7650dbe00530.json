AKH,OAAzB,EAAkC,KAAKjB,SAAL,CAAeW,QAAjD;AACH,KAJG;AAMJU,IAAAA,IAAI,EAAC,YAAW;AACZ,WAAK7B,MAAL,GAAc,KAAd;AACA,WAAK4B,KAAL,CAAW,QAAX,EAAqB,KAAKpB,SAA1B;AACH,KATG;AAWJsB,IAAAA,WAXI,cAWU;AAAA;;AACVC,MAAAA,MAAM,CAACC,IAAP,CAAY,eAAZ,EAA6B,KAAKP,OAAlC,EAA2C,KAAKjB,SAAL,CAAeM,KAA1D,EAAiE,UAACmB,KAAD,EAAQC,MAAR,EAAmB;AAAE,QAAA,KAAI,CAACP,IAAL;AAAc,OAApG;AACH,KAbG;AAeJQ,IAAAA,WAfI,cAeU;AACV;AACAJ,MAAAA,MAAM,CAACC,IAAP,CAAY,eAAZ,EAA6B,KAAKP,OAAlC,EAFU,CAIV;;AACA,WAAKG,KAAL,CAAW,SAAX,EAAsB,KAAKH,OAA3B;AACH;AArBG;AA9EG,CANf","sourcesContent":["\n\n//collections\nimport { Frames } from \"../../imports/collections/frames.js\";\n\n\nexport default {\n    name: \"Frame\",\n\n    props: [\"frameId\", \"masks\"],\n    \n    data() {\n        return {\n            isOpen:false, \n            maskVisibility:'hidden',\n        };\n    },\n\n    \n    watch: {\n        masks: {\n            deep:true,\n            handler() {\n                console.log(\"masks changed\")\n\n                if (this.frameData) {\n                    let tag = this.frameData.tag;\n                    let visibility = this.masks[tag].value;\n\n\n                    if (visibility) {\n                        this.maskVisibility = 'visible'\n                    }\n                    else {\n                        this.maskVisibility = 'hidden'\n                    }\n                }\n\n            }\n        }\n\n    },\n    \n\n\n    computed: {\n        dynamicStyle: function () {\n           \n            if (!this.frameData)\n                return {}\n            else {\n                return {\n                    'border-color': 'rgb(' + this.frameData.color.r+ ',' + this.frameData.color.g+ ',' + this.frameData.color.b+')',\n                }\n            }\n        },\n\n        hasChildren: function() {\n            if (!this.frameData)\n                return false;\n\n            return this.frameData.children.length > 0;\n        },\n\n        dynamicMaskStyle: function() {\n            if (!this.frameData)\n                return {'visibility':'hidden'}\n            return {\n                'visibility' : this.maskVisibility,\n                'background-color' : this.frameData.tag == 0? 'rgba(255,0,0,0.9)':'rgba(0,0,255,0.9)',\n                }\n        }\n            \n    },\n\n    meteor: {\n        //responsive frames collection\n        frameData() {\n            return Frames.findOne({_id:this.frameId});\n        },\n\n    },\n\n\n    methods:{\n        show:function() {\n            this.isOpen = true;\n            this.$emit('shown', this.frameId, this.frameData.children)\n        },\n\n        hide:function() {\n            this.isOpen = false;\n            this.$emit('hidden', this.frameData)\n        },\n\n        createFrame() {\n            Meteor.call(\"frames.insert\", this.frameId, this.frameData.color, (error, result) => { this.show(); }); \n        },\n\n        deleteFrame() {\n            //delete in DB\n            Meteor.call(\"frames.remove\", this.frameId);\n\n            //also delete from app viex\n            this.$emit('deleted', this.frameId);\n        },\n  }\n\n};\n"]},"sourceType":"module","hash":"562fa990c3e2a3dda7683fd4c39f7650dbe00530"}
